{
  "name": "qde-agent",
  "description": "AI Agent system for QDE (Quick Data Entry) trade operations using PocketFlow framework",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/qde-agent.git"
  },
  "keywords": [
    "qde",
    "agent",
    "trading",
    "energy",
    "pocketflow",
    "typescript",
    "llm",
    "ai",
    "mcp"
  ],
  "scripts": {
    "build": "tsup",
    "build-mcp": "cd mcp/customer-server && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsx src/index.ts",
    "demo": "tsx src/demo-enhanced-infrastructure.ts",
    "mcp-server": "tsx mcp/server/index.ts",
    "mock-api": "tsx mock-api/server.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/express": "^5.0.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
